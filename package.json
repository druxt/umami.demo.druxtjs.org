    "@nuxtjs/lunr-module": "^0.3.2",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.1",
    "core-js": "^3.8.1",
    "css-loader": "^5.2.5",
    "druxt": "^0.6.0",
    "druxt-entity": "^0.13.0",
    "druxt-menu": "^0.10.0",
    "druxt-search": "^0.1.1",
    "druxt-site": "^0.4.0",
    "druxt-views": "^0.9.0",
    "jsonapi-serializer": "^3.6.6",
