{
  "name": "demo.druxtjs.org",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "chromatic": "npx chromatic --build-script-name storybook:build",
    "cy:run": "cypress run",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "serve": "npm run generate && npm start",
    "start": "nuxt start",
    "storybook:build": "API_PROXY=0 npx nuxt storybook build",
    "storybook": "npx nuxt storybook",
    "test:cy": "start-server-and-test serve http://localhost:3000 cy:run",
    "test": "npm run test:cy"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/lunr-module": "0.3.2",
    "bootstrap": "5.1.3",
    "bootstrap-vue": "2.22.0",
    "core-js": "3.22.5",
    "css-loader": "5.2.7",
    "druxt-search": "0.1.1",
    "druxt-site": "^0.12.0",
    "jsonapi-serializer": "3.6.7",
    "nuxt": "2.15.8",
    "vue-country-flag": "^2.3.2",
    "vue-json-pretty": "1.8.3",
    "vue-live": "1.17.2",
    "vuex": "3.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@cypress/vue": "2.2.4",
    "@cypress/webpack-dev-server": "1.8.4",
    "@nuxtjs/eslint-config": "9.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/storybook": "4.2.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "chromatic": "6.5.4",
    "cypress": "9.6.1",
    "cypress-cucumber-preprocessor": "4.3.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "html-webpack-plugin": "4.5.2",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "postcss": "8.4.13",
    "postcss-html": "1.4.1",
    "prettier": "2.6.2",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.8.2",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "25.0.0",
    "webpack-dev-server": "4.9.0"
  }
}
