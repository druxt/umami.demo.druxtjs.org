{
  "name": "demo.druxtjs.org",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "chromatic": "npx chromatic --build-script-name storybook:build",
    "cy:run": "cypress run",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "serve": "npm run generate && npm start",
    "start": "nuxt start",
    "storybook:build": "API_PROXY=0 npx nuxt storybook build",
    "storybook": "npx nuxt storybook",
    "test:cy": "start-server-and-test serve http://localhost:3000 cy:run",
    "test": "npm run test:cy"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1643626889.31bf6df",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/lunr-module": "0.3.2",
    "bootstrap": "5.1.3",
    "bootstrap-vue": "2.21.2",
    "core-js": "3.20.3",
    "css-loader": "5.2.7",
    "druxt-search": "0.1.1",
    "druxt-site": "0.10.5",
    "jsonapi-serializer": "3.6.7",
    "nuxt": "2.15.8",
    "vue-json-pretty": "1.8.2",
    "vue-live": "1.17.2",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.5",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@cypress/vue": "^2.2.3",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@nuxtjs/eslint-config": "8.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/storybook": "4.2.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "chromatic": "6.4.3",
    "cypress": "^9.4.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.4.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "7.0.4",
    "lint-staged": "12.3.2",
    "postcss": "8.4.5",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.1.0",
    "stylelint-config-standard": "24.0.0"
  }
}
